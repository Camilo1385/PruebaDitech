@model List<Seller>

<h1>Lista de Ciudades</h1>

<table border="1">
    <thead>
        <tr>
            <th>Codigo</th>
            <th>Descripcion</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var seller in Model)
        {
        <tr>
            <td>@seller.Document</td>
            <td>@seller.Name</td>
            <td>@seller.LastName</td>
        </tr>
        }
    </tbody>
</table>